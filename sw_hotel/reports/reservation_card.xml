<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="reservation_card">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <h1>Reservation Card</h1>
                    <p>Reservation ID: <t t-esc="doc.name"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>
